[remote "EachenKuang"]
    url = https://github.com/EachenKuang/LeetCode.git
    fetch = +refs/heads/*:refs/remotes/EachenKuang/*
[remote "origin"]
    url = git@github.com:huangyingw/eachenkuang_leetcode.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = EachenKuang
    branch = master
[merge]
	allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[commit]
	message = n
[vrun]
	silent = false
	async = false
[deploy]
	host = localhost
	path = ~/myproject/git/interview/leetcode_solutions/EachenKuang/LeetCode
